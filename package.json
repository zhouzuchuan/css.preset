{
    "name": "css.preset",
    "version": "0.2.0",
    "description": "css preset",
    "main": "preset.min.css",
    "style": "preset.min.css",
    "scripts": {
        "build": "lessc preset.less > preset.css  --autoprefix='last 2 version' ",
        "prebuild": "lessc preset.less > preset.min.css  --autoprefix='last 2 version' --clean-css='--s1 --advanced --compatibility=ie8'",
        "prepublish": "npm run build",
        "version": "npm run build",
        "postversion": "git add . && git commit -am '' && git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zhouzuchuan/css.preset.git"
    },
    "keywords": [
        "css",
        "preset"
    ],
    "author": "fezzc",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/zhouzuchuan/css.preset/issues"
    },
    "homepage": "https://github.com/zhouzuchuan/css.preset#readme",
    "devDependencies": {
        "less": "^4.1.2",
        "less-plugin-autoprefix": "^2.0.0",
        "less-plugin-clean-css": "^1.5.1"
    }
}
